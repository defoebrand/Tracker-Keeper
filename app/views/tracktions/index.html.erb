<p id="notice"><%= notice %></p>

<h1>Tracktions</h1>

<table>
  <thead>
    <tr>
      <th>Author</th>
      <th>Type</th>
      <th>Name</th>
      <th>Amount</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tracktions.each do |tracktion| %>
      <tr>
        <td><%= tracktion.author_id %></td>
        <td><%= tracktion.type_id %></td>
        <td><%= tracktion.name %></td>
        <td><%= tracktion.amount %></td>
        <td><%= link_to 'Show', tracktion %></td>
        <td><%= link_to 'Edit', edit_tracktion_path(tracktion) %></td>
        <td><%= link_to 'Destroy', tracktion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tracktion', new_tracktion_path %>
